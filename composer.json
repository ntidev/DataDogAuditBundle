{
  "name": "nti/audit-bundle",
  "type": "symfony-bundle",
  "description": "Audit bundle for symfony3 and doctrine orm, logs any database change",
  "keywords": ["audit", "log", "symfony3", "bundle", "doctrine", "orm"],
  "license": "MIT",

  "authors": [
    {
      "name": "DataDog Team",
      "homepage": "http://datadog.lt"
    },
    {
      "name": "Symfony2 Community",
      "homepage": "http://github.com/DATA-DOG/DataDogAuditBundle/contributors"
    }
  ],

  "require": {
    "php": ">=5.4.0",
    "symfony/framework-bundle": "^2.6|^3.0|^4.0|^4.4|^5.4|^6.4",
    "doctrine/orm": "^2.4"
  },

  "require-dev": {
    "php": ">=5.6.0",
    "ext-pdo": "*",
    "ext-pdo_sqlite": "*",

    "symfony/symfony": "^3.0|^4.0|^4.4|^5.4|^6.4",
    "doctrine/orm": "^2.5|^3.0",
    "doctrine/doctrine-bundle": "^1.6|^2.0",
    "doctrine/doctrine-fixtures-bundle": "^2.2",
    "sensio/framework-extra-bundle": "^3.0.2|^5.1",
    "knplabs/knp-menu-bundle": "^2.0",
    "knplabs/knp-time-bundle": "^1.3",
    "data-dog/pager-bundle": "^0.2",

    "phpunit/phpunit": "~4.7.0|^5.1"
  },

  "autoload": {
    "psr-4": {
      "DataDog\\AuditBundle\\": "src/DataDog/AuditBundle"
    }
  },

  "autoload-dev": {
    "psr-4": {
      "": "example/src/"
    }
  },

  "archive": {
    "exclude": ["vendor", "tests", "*phpunit.xml", ".travis.yml", "example", "Makefile", "screenshots"]
  },

  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  }
}
